{
  "name": "puppetfinland-buildbot",
  "version": "2.2.0",
  "author": "Puppet-Finland team",
  "summary": "Setup buildbot masters and slaves",
  "license": "BSD-2-Clause",
  "source": "https://github.com/Puppet-Finland/puppet-buildbot.git",
